{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "api/script.js"
      },
      {
        "src": "/public/(.*)",
        "dest": "/public/$1"
      },
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/css/(.*)",
        "dest": "CSS Files/$1"
      },
      {
        "src": "/html/(.*)",
        "dest": "HTML Files/$1"
      },
      {
        "src": "/js/(.*)",
        "dest": "JS Files/$1"
      },
      {
        "src": "/covers/(.*)",
        "dest": "covers/$1"
      },
      {
        "src": "/songs/(.*)",
        "dest": "songs/$1"
      }
    ]
}
